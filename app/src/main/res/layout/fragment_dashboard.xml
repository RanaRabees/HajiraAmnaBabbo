<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#AB47BC"
    tools:context=".ui.dashboard.DashboardFragment">




    <ScrollView
        android:layout_width="409dp"
        android:layout_height="729dp"
        tools:ignore="MissingConstraints,SpeakableTextPresentCheck"
        tools:layout_editor_absoluteX="1dp"
        tools:layout_editor_absoluteY="1dp">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">
            <TextView
                android:id="@+id/textView"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:background="#C0AD5D"
                android:fontFamily="monospace"
                android:text="This is My Cute                  ðŸ˜ŠAmna Baby pageðŸ˜˜"
                android:textAlignment="center"
                android:textColor="#CC3030"
                android:textSize="20sp"
                android:textStyle="bold|italic"
                tools:ignore="TextContrastCheck" />


            <ImageView
                android:id="@+id/imageView1"
                android:layout_width="408dp"
                android:layout_height="366dp"
                android:src="@drawable/amc" />

            <ImageView
                android:id="@+id/imageView2"
                android:layout_width="408dp"
                android:layout_height="429dp"
                android:src="@drawable/ame" />

            <ImageView
                android:id="@+id/imageView3"
                android:layout_width="match_parent"
                android:layout_height="420dp"
                android:src="@drawable/ama" />

            <ImageView
                android:id="@+id/imageView4"
                android:layout_width="match_parent"
                android:layout_height="420dp"
                android:layout_marginBottom="150dp"
                android:src="@drawable/amf" />

        </LinearLayout>
    </ScrollView>


</androidx.constraintlayout.widget.ConstraintLayout>